{
  "version": 2,
  "routes": [
    { "src": "/api/categories", "dest": "/api/categories.js" }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["api/**"],
        "internalServer": true
      }
    }
  ]
}